<% if @venue %>
  <table border="0" cellpadding="0" cellspacing="0" width="100%">
    <tr>
      <td align="center">
        <p class="addressText"><%= @venue.venue_name %>: <%= @venue.phone_number %></p>
      </td>
    </tr>
    <tr>
      <td align="center">
        <p class="addressText"><%= @venue.venue_address %></p>
      </td>
    </tr>
  </table>
<% end %>
