<% if @venue %>
  <h2 class="aligncenter"><%= @venue.venue_name %></h2>
  <table border="0" cellpadding="0" cellspacing="0" width="100%">
    <tr>
      <td align="center">
        <p class="titleAddressText mt10"><%= @venue.phone_number %></p>
      </td>
    </tr>
    <tr>
      <td align="center">
        <p class="titleAddressText"><%= @venue.venue_address %></p>
      </td>
    </tr>
  </table>
<% end %>
